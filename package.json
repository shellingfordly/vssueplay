{
  "name": "vssueplay",
  "version": "0.0.0",
  "description": "A Vue3-powered Issue-based Comment Plugin",
  "main": "index.js",
  "scripts": {
    "prepare": "simple-git-hooks",
    "demo": "pnpm --filter @vssueplay/demos",
    "docs:dev": "pnpm --filter @vssueplay/docs",
    "comps": "pnpm --filter @vssueplay/comps",
    "dev": "pnpm --recursive --parallel --stream run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "vitest --sequence.setupFiles list --environment jsdom"
  },
  "keywords": [],
  "author": "shellingfordly <shellingfordly@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@types/node": "^20.12.7",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^9.0.0",
    "lint-staged": "^15.2.2",
    "picocolors": "^1.0.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.2.2",
    "vitest": "^1.5.0"
  }
}